<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lf.lfengalbum">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!--Rxjava简介-->
        <activity android:name=".ui.rxjava.RxjavaIntroduceActivity"/>
        <!--基本使用-->
        <activity android:name=".ui.rxjava.RxjavaUseCasesActivity"/>
        <!--操作符-->
        <activity android:name=".ui.rxjava.RxjavaOperatorActivity"/>

        <activity android:name=".ui.basis.BasisAdbActivity"/>
    </application>


</manifest>
